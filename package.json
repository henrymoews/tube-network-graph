{
  "name": "@henrymoews/workspace",
  "version": "0.1.0",
  "description": "A tool to easily create tube-like network maps based on graphviz.",
  "scripts": {
    "compile": "standard --fix && babel --presets es2015,stage-0 -d bin/ src/",
    "watch": "standard --fix && babel --watch --presets es2015,stage-0 -d bin/ src/",
    "prepublish": "npm run compile",
    "test": "standard --fix && echo \"Error: no test specified\" && exit 1",
    "tube-network-graph": "tube-network-graph"
  },
  "bin": {
    "tube-network-graph": "bin/tube-network-graph.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrymoews/tube-network-graph.git"
  },
  "keywords": [
    "tube",
    "network",
    "graph",
    "graphviz",
    "bus",
    "train",
    "stops",
    "stations"
  ],
  "author": "Henry Moews",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/henrymoews/tube-network-graph/issues"
  },
  "homepage": "https://github.com/henrymoews/tube-network-graph#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "get-stdin": "^5.0.1",
    "shelljs": "^0.7.8",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "node-jsxml": "^0.9.0"
  }
}
